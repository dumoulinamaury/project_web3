
		<nav>
			<ul class="header-nav-primary nav nav-pills collapse navbar-collapse">
			{% for d in nav %}
				{% if d['access'] == "public" %}
				<li>
					<a href="#">{{ d['name'] }} <i class="fa fa-chevron-down"></i></a>
					<ul class="sub-menu">
						{% for page in d['pages'] %}
						<li><a href="{{ path(page['route']) }}">{{ page['name'] }}</a></li>
						{% endfor %}
					</ul>
				</li>
				{% endif %}
			{% endfor %}
			</ul>
		</nav>
